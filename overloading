#include<iostream>
#include<conio.h>
using namespace std;
class FLOAT1
{
	float b;
	public:
		FLOAT1(float b)
		{
			this->b=b;
		}
		float operator+(FLOAT1 F)
		{
			return(b+F.b);
		}
		float operator-(FLOAT1 F)
		{
			return(b-F.b);
		}
		float operator*(FLOAT1 F)
		{
			return(b*F.b);
		}
		float operator/(FLOAT1 F)
		{
			return(b/F.b);
		}
		  		
};
int main()
{
	FLOAT1 F1(83.87),F2(30.72);
	cout<<"\n\n Overloading Operator + : "<<F1+F2;
	cout<<"\n\n Overloading Operator - : "<<F1-F2;
	cout<<"\n\n Overloading Operator * : "<<F1*F2;
	cout<<"\n\n Overloading Operator / : "<<F1/F2;
return 0;
}
