#include<iostream>
#include<conio.h>
using namespace std;
class FLOAT1
{
	float b;
	public:
		FLOAT1(float b)
		{
			this->b=b;
		}
		float operator+(FLOAT1 F)
		{
			return(b+F.b);
		}
		float operator-(FLOAT1 F)
		{
			return(b-F.b);
		}
		float operator*(FLOAT1 F)
		{
			return(b*F.b);
		}
		float operator/(FLOAT1 F)
		{
			return(b/F.b);
		}
		  		
};
int main()
{
	FLOAT1 f1(83.87),f2(30.72);
	cout<<"\n\n Operator + : "<<f1+f2;
	cout<<"\n\n Operator - : "<<f1-f2;
	cout<<"\n\n Operator * : "<<f1*f2;
	cout<<"\n\n Operator / : "<<f1/f2;
return 0;
}
